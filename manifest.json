{
    "manifest_version": 2,
    "name": "Youtube-AutoReplay",
    "description": "automatically replays youtube video.",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "inject.js"
    ]
}